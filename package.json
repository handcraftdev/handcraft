  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@tamagui/animations-css": "^1.138.6",
    "@tamagui/config": "^1.138.6",
    "@tamagui/core": "^1.138.6",
    "@tamagui/font-inter": "^1.138.6",
    "@tamagui/themes": "^1.138.6",
    "tamagui": "^1.138.6"
  }
