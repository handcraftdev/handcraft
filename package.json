{
  "name": "handcraft",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:web": "turbo dev --filter=@handcraft/web",
