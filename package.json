{
  "name": "handcraft",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:web": "turbo dev --filter=@handcraft/web",
    "dev:web:prod": "turbo build --filter=@handcraft/web && pnpm --filter=@handcraft/web start",
    "dev:mobile": "turbo dev --filter=@handcraft/mobile",
    "build:web": "turbo build --filter=@handcraft/web",
    "build:mobile": "turbo build --filter=@handcraft/mobile",
    "test:e2e": "pnpm --filter=@handcraft/web test:e2e",
    "test:e2e:ui": "pnpm --filter=@handcraft/web test:e2e:ui",
    "test:e2e:headed": "pnpm --filter=@handcraft/web test:e2e:headed",
    "test:e2e:debug": "pnpm --filter=@handcraft/web test:e2e:debug"
  },
  "devDependencies": {
    "next": "^16.1.0",
    "@metaplex-foundation/mpl-core": "^1.7.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@metaplex-foundation/umi-web3js-adapters": "^1.4.1",
    "@noble/hashes": "1.3.3",
    "tsx": "^4.21.0",
    "turbo": "^2.3.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@tamagui/animations-css": "^1.138.6",
    "@tamagui/config": "^1.138.6",
    "@tamagui/core": "^1.138.6",
    "@tamagui/font-inter": "^1.138.6",
    "@tamagui/themes": "^1.138.6",
    "tamagui": "^1.138.6"
  }
}
