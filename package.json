{
  "name": "@worldcoin/next-15-template",
  "version": "0.1.0",
  "homepage": "https://docs.worldcoin.org/mini-apps",
  "description": "Create a Mini App",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "db:migrate": "./supabase/apply-migrations.sh",
    "db:migrate:js": "node scripts/apply-migrations.js",
    "dev:local": "NEXT_PUBLIC_ENV=local next dev",
    "build:dev": "NEXT_PUBLIC_ENV=development next build",
    "build:stag": "NEXT_PUBLIC_ENV=staging next build",
    "build:prod": "NEXT_PUBLIC_ENV=production next build",
    "start:dev": "NEXT_PUBLIC_ENV=development next start",
    "start:stag": "NEXT_PUBLIC_ENV=staging next start",
    "start:prod": "NEXT_PUBLIC_ENV=production next start"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@worldcoin/mini-apps-ui-kit-react": "^1.0.2",
    "@worldcoin/minikit-js": "latest",
    "@worldcoin/minikit-react": "latest",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "iconoir-react": "^7.11.0",
    "next": "15.2.3",
    "next-auth": "^5.0.0-beta.25",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.2.0",
    "viem": "2.23.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eruda": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4.1.5",
    "typescript": "^5"
  }
}
